---
const { navn, email, rolle, billede } = Astro.props;
---

<article>
  <img src={`images/${billede}`} alt={navn} />
  <h3>{navn}</h3>
  <p>{email}</p>
  <p>{rolle}</p>
</article>

<style>
  article {
    background-color: antiquewhite;
    padding: 1rem;
  }
  img {
    max-width: 100%;
  }
</style>
